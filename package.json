{
  "name": "react-discussion-component",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./bin/www.js",
    "debug": "cross-env DEBUG=react-discussion-component:* nodemon ./bin/www.js",
    "build-css": "tailwindcss -i ./public/stylesheets/style.css -o ./public/dist/output.css",
    "build-css:dev": "tailwindcss -i ./public/stylesheets/style.css -o ./public/dist/output.css --watch",
    "lint": "standard --env jest",
    "unittest": "jest",
    "test": "npm run lint && npm run unittest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "standard": "^17.0.0",
    "tailwindcss": "^3.1.8"
  }
}
